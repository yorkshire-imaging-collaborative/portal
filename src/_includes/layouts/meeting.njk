{% set startDate = start | dateFormat('dd LLLL yyyy') %}
{% set endDate = end | dateFormat('dd LLLL yyyy') %}

<span class="font-bold">{{ start | dateFormat('dd LLLL yyyy HH:mm') }}</span>
{% if startDate == endDate %}
    — <span class="font-bold">{{ end | dateFormat('HH:mm') }}</span>
    ({{ start | duration(end) }})
{% else %}
    — <span class="font-bold">{{ end | dateFormat('dd LLLL yyyy HH:mm') }}</span>
{% endif %}

<h2>Agenda</h2>
{{ agenda | safe }}

<h2>Minutes</h2>
{{ minutes | safe }}