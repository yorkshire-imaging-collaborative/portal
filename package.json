{
  "name": "nhs-11ty",
  "version": "1.0",
  "description": "A starter for making Eleventy websites for the NHS",
  "main": "index.js",
  "author": "danfascia",
  "license": "MIT",
  "scripts": {
    "tailwind": "tailwind build ./src/styles/styles.css -o ./dist/styles.css",
    "dev": "eleventy --serve",
    "build": "export NODE_ENV=production && rm -rf dist && eleventy && tailwind build ./src/styles/styles.css -o ./dist/styles.css"
  },
  "dependencies": {
    "@11ty/eleventy": "0.11.1",
    "@11ty/eleventy-navigation": "0.1.4",
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/line-clamp": "^0.2.0",
    "@tailwindcss/typography": "^0.3.1",
    "autoprefixer": "^10.1.0",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "eleventy-plugin-nesting-toc": "^1.3.0",
    "html-minifier": "^4.0.0",
    "markdown-it": "^12.0.4",
    "markdown-it-anchor": "^7.0.2",
    "postcss": "^8.2.2",
    "tailwindcss": "2.0.2"
  }
}
